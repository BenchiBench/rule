name: 百度网盘
desc: 解锁解锁在线视频倍率/清晰度

http:
  mitm:
    - "pan.baidu.com"
  script:
    - match: https:\/\/pan\.baidu\.com\/rest\/\d\.\d\/membership\/user
      name: BaiduCloud
      type: response
      require-body: true
      timeout: 10
      
script-providers:
  BaiduCloud:
    url: https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/BaiduCloud.js
    interval: 3600
